    <title>Homepage</title>
    @if (Session["lang"].ToString() == "en")
    {
        <link rel="stylesheet" type="text/css" href=@Url.Content( "~/en/css/bootstrap.css") />
        <link rel="stylesheet" type="text/css" href=@Url.Content( "~/en/css/font-awesome.css") />
