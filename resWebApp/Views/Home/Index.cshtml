<script>
    function refreshFunc() {
        var bId = $('#branchId').val();
        if (bId == "")
            bId = 0;
        $.ajax({
            url: '@Url.Action("RefreshDashBoard", "Home")',
            data: { branchId: bId },
            type: "POST",
            async: false,
            success: function (result) {
                $('#sales').text(result.salesCount);
                $('#tables').text(result.tablesCount);
                $('#dining-hall').text(result.dinningHallCount);
                $('#self-service').text(result.selfServiceCount);
                $('#takeaway').text(result.takeawayCount);
                $('#reservations').text(result.reservationsCount);
                $('#users').text(result.onLineUsersCount);
                $('#cash').text(result.balance);
            },
            error: function(XMLHttpRequest, textStatus, errorThrown) {
        alert('oops, something bad happened');
    }
        });
        }
</script>